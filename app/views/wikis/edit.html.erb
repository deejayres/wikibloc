<div class="container">
  <div class="authform">
    <%= form_for @wiki do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :body %>
        <%= f.text_area :body, class: "form-control" %>
      </div>

      <div class="form=group">
        <%= f.submit "Save", class: "form-control btn btn-success" %>
      </div>
    <% end %>
  </div>
</div>
